// Input: n
// Output: res = n^2
// Description: Squares the input using a while loop

{ n > 0 }

res := 0;

{ res = 0 ∧ n > 0 }

i := 0;

{ i = 0 ∧ res = 0 ∧ n > 0 }

while i < n [ 
    res = i*n ∧ i <= n ∧ n > 0
] do
    
    res := res + n;
    
    { res = (i+1)*n ∧ i < n ∧ n > 0 }
    
    i := i + 1;
;

{ res = n * n ∧ i = n ∧ n > 0 }

